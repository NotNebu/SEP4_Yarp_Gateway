{
  "ReverseProxy": {
    "Routes": {
      "authDirect": {
        "ClusterId": "userService",
        "Match": {
          "Path": "/AuthService/{**catch-all}"
        }
      },
      "userGrpc": {
        "ClusterId": "userService",
        "Match": {
          "Path": "/user.grpc.AuthService/{**catch-all}"
        }
      },
      "authRoute": {
        "ClusterId": "userService",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        }
      },
      "accountRoute": {
        "ClusterId": "userService",
        "Match": {
          "Path": "/api/account/{**catch-all}"
        },
        "AuthorizationPolicy": "RequireAuth"
      },
      "experimentRoute": {
        "ClusterId": "userService",
        "Match": {
          "Path": "/api/experiment/{**catch-all}"
        },
        "AuthorizationPolicy": "RequireAuth"
      },
      "modelTrainingRoute": {
        "ClusterId": "modelTrainingService",
        "Match": {
          "Path": "/ModelTraining/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "userService": {
        "Destinations": {
          "destination1": {
            "Address": "https://user-service:5001"
          }
        }
      },
      "modelTrainingService": {
        "Destinations": {
          "destination1": {
            "Address": "http://API-container:8080"
          }
        }
      }
    }
  }
}
